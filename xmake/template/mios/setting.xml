<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
    http://maven.apache.org/xsd/settings-1.0.0.xsd">
    
    <localRepository></localRepository>
    <interactiveMode>false</interactiveMode>
    <usePluginRegistry>false</usePluginRegistry>
    <offline>false</offline>
    
  <mirrors>
    <mirror>
      <mirrorOf>*,!central,!build.milestones.ios.plugin.repo,!build.milestones.ios.repo,!build.releases.ios.plugin.repo,!build.releases.ios.repo,!build.snapshots.ios.repo,!deploy.milestones.ios.plugin.repo,!deploy.milestones.ios.repo,!deploy.releases.ios.plugin.repo,!deploy.releases.ios.repo</mirrorOf>
      <url></url>
      <id>mirror1</id>
    </mirror>
  </mirrors>
  
  <profiles>
    <profile>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
      </properties>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases/</url>
        </repository>
        <repository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>build.releases.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases.ios/</url>
        </repository>
        <repository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.releases.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases.ondevice/</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>build.releases.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases.ios/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.releases.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.releases.ondevice/</url>
        </pluginRepository>
      </pluginRepositories>
      <id>release.build</id>
    </profile>
    <profile>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
      </properties>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones/</url>
        </repository>
        <repository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>build.milestones.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ios/</url>
        </repository>
        <repository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.milestones.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ondevice/</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>build.milestones.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ios/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.milestones.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ondevice/</url>
        </pluginRepository>
      </pluginRepositories>
      <id>milestone.build</id>
    </profile>
    <profile>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
      </properties>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.snapshots/</url>
        </repository>
        <repository>
          <snapshots>
            <updatePolicy>always</updatePolicy>
            <checksumPolicy>fail</checksumPolicy>
          </snapshots>
          <id>build.snapshots.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.snapshots.ios/</url>
        </repository>
        <repository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.milestones.ios.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ondevice/</url>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://nexus:8081/nexus/content/groups/build.snapshots/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>build.milestones.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ios/</url>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <checksumPolicy>fail</checksumPolicy>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>deploy.milestones.ios.plugin.repo</id>
          <url>http://nexus:8081/nexus/content/groups/build.milestones.ondevice/</url>
        </pluginRepository>
      </pluginRepositories>
      <id>snapshot.build</id>
    </profile>
    <profile>
      <activation>
        <activeByDefault>true</activeByDefault>
      </activation>
      <properties>
        <tycho.disableP2Mirrors>true</tycho.disableP2Mirrors>
        <sonar.jdbc.url>jdbc:mysql://ldisonarci.wdf.sap.corp:3306/sonar?useUnicode=true&amp;characterEncoding=utf8</sonar.jdbc.url>
        <sonar.jdbc.password>sonar</sonar.jdbc.password>
        <sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
        <tycho.localArtifacts>ignore</tycho.localArtifacts>
        <sonar.host.url>http://ldisonarci.wdf.sap.corp:8080/sonar</sonar.host.url>
        <sonar.jdbc.username>sonar</sonar.jdbc.username>
      </properties>
      <id>sonar</id>
    </profile>
    <profile>
      <properties>
        <xcode.codeSignIdentity>iPhone Distribution: SAP AG</xcode.codeSignIdentity>
        <xcode.settings.GCC_GENERATE_DEBUGGING_SYMBOLS>YES</xcode.settings.GCC_GENERATE_DEBUGGING_SYMBOLS>
        <xcode.app.defaultSdks>iphoneos</xcode.app.defaultSdks>
        <mios.ota-service.url>https://apple-ota.wdf.sap.corp:8443/ota-service/HTML</mios.ota-service.url>
	<mios.ota-service.buildHtmlTemplate>${env.HOME}/otaBuildTemplate.html</mios.ota-service.buildHtmlTemplate>
        <xcode.settings.DEBUG_INFORMATION_FORMAT>dwarf-with-dsym</xcode.settings.DEBUG_INFORMATION_FORMAT>
        <xcode.app.defaultConfigurations>Release</xcode.app.defaultConfigurations>
        <xcode.failOnMissingSyncInfo>true</xcode.failOnMissingSyncInfo>
        <xcode.provisioningProfile></xcode.provisioningProfile>
        <xcode.useSymbolicLinks>true</xcode.useSymbolicLinks>
        <xcode.appIdSuffix>internal</xcode.appIdSuffix>
      </properties>
      <id>central.enterprise.build</id>
        <activation>
            <activeByDefault>true</activeByDefault>
        </activation>

    </profile>
    <profile>
      <properties>
        <xcode.codeSignIdentity>iPhone Distribution</xcode.codeSignIdentity>
        <xcode.settings.GCC_GENERATE_DEBUGGING_SYMBOLS>YES</xcode.settings.GCC_GENERATE_DEBUGGING_SYMBOLS>
        <xcode.app.defaultSdks>iphoneos</xcode.app.defaultSdks>
        <mios.ota-service.url>http://apple-ota.wdf.sap.corp:8080/ota-service/HTML</mios.ota-service.url>
        <xcode.settings.DEBUG_INFORMATION_FORMAT>dwarf-with-dsym</xcode.settings.DEBUG_INFORMATION_FORMAT>
        <xcode.app.defaultConfigurations>Release</xcode.app.defaultConfigurations>
        <xcode.forbidLibBuild>true</xcode.forbidLibBuild>
        <xcode.provisioningProfile></xcode.provisioningProfile>
        <xcode.failOnMissingSyncInfo>true</xcode.failOnMissingSyncInfo>
        <xcode.useSymbolicLinks>true</xcode.useSymbolicLinks>
        <xcode.appIdSuffix>release</xcode.appIdSuffix>
        <xcode.artifactIdSuffix>release</xcode.artifactIdSuffix>
      </properties>
      <id>central.naas.build</id>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>snapshot.build</activeProfile>
  </activeProfiles>
  <pluginGroups>
    <pluginGroup>com.sap.ldi</pluginGroup>
  </pluginGroups>
</settings>
